{
  "cleanupPeriodDays": 720,
  "env": {
    "CLAUDE_BASH_TIMEOUT_MS": "600000",
    "CLAUDE_MAX_BASH_TIMEOUT_MS": "86400000",
    "CLAUDE_MAX_OUTPUT_TOKENS": "42069",
    "CLAUDE_CODE_MAX_THINKING_TOKENS": "32000"
  },
  "permissions": {
    "allow": [
      "WebSearch",
      "Bash(gh:*)",
      "Bash(git:*)",
      "Bash(tree:*)",
      "Bash(npm run build:*)"
    ],
    "deny": [],
    "defaultMode": "default"
  },
  "model": "opus",
  "hooks": {},
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.sh"
  },
  "enabledPlugins": {
    "claude-mem@thedotmack": true
  },
  "spinnerTipsEnabled": false,
  "alwaysThinkingEnabled": true,
  "autoUpdatesChannel": "latest",
  "promptCaching": true
}
